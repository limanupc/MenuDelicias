<%- model_class = Correlative -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:Branch_id) %>:</strong></dt>
  <dd><%= @correlative.Branch_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:document_type_id) %>:</strong></dt>
  <dd><%= @correlative.document_type_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:series) %>:</strong></dt>
  <dd><%= @correlative.series %></dd>
  <dt><strong><%= model_class.human_attribute_name(:number) %>:</strong></dt>
  <dd><%= @correlative.number %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              correlatives_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_correlative_path(@correlative), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              correlative_path(@correlative),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
